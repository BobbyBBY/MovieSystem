<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moviesystemclient">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
        </activity>
        <activity
            android:name=".activities.LunchActivity"
            android:theme="@style/LunchTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.OrderActivity">
        </activity>
        <activity
            android:name=".activities.ScreeningActivity">
        </activity>
        <activity
            android:name=".activities.SeatActivity">
        </activity>
        <activity
            android:name=".activities.TicketActivity">
        </activity>
        <activity
            android:name=".activities.webview">
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>